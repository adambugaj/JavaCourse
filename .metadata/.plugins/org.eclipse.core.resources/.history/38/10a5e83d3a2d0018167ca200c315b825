package PasswordGenerator;

import java.util.*;

public class PasswordGenerator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		listUsers();
	}
	
	private static void listUsers() {
		Password user1 = new Password("Adam", "PR");
		Password user2 = new Password("Greg", "Media");
		
		
		
		List<Password> userList = new LinkedList<>();
		userList.add(user1);
		userList.add(user2);
		
		System.out.println(userList.get(0));
		user1.showData();
		
		
	}
}
