package Figures;

public class Rectangle extends Figure {

		private int width;
		private int height;
		
		public Rectangle(int x, int y, int width, int height) {
			super(x, y);
			this.width = width;
			this.height = height;
		}
		
		public void draw() {
			System.out.println(this.width + this.height);
		}
		

}
